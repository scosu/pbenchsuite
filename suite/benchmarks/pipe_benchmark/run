#!/usr/bin/python3

import subprocess
import time
import sys
import json

forks = int(sys.argv[1])

ps = []
start = time.time()
for i in range(0, forks):
	ps.append(subprocess.Popen(['./pipe-test']))

for i in ps:
	i.wait()
end = time.time()
print(json.dumps({'runtime': {'value': (end - start - 5), 'unit': 's', 'description': 'runtime'}}))
